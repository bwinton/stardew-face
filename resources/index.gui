<svg id="root">
	<svg width="305" height="305">
		<image href="map/water1.png" id="water" />
		<image href="map/summer.png" id="map" />
	</svg>

	<svg id="clockElement">
		<svg x="5" y="0" width="293" height="240">
			<image href="clock/clock.png" />
			<image href="clock/season/summer.png" id="season" width="50" height="33" x="215" y="65" />

			<rect x="118" y="64" width="49" height="34" fill="white" />
			<use href="#fitfont" id="heartRateTextShadow">
				<use href="#fitfont-char"/>
				<use href="#fitfont-char"/>
				<use href="#fitfont-char"/>
			</use>
			<use href="#fitfont" id="heartRateText">
				<use href="#fitfont-char"/>
				<use href="#fitfont-char"/>
				<use href="#fitfont-char"/>
			</use>
			<svg>
				<defs>
					<symbol id="heart">
						<g id="heartGroup">
							<image href="clock/weather/wedding.png" width="48" height="32" x="119" y="65" />
							<animate id="heartAnimationFade" attributeName="opacity" begin="enable" from="1" to="0" dur="0.5" final="keep" />
							<animate id="heartAnimationRise" attributeName="opacity" begin="enable" from="0" to="1" dur="0.5" final="keep" />
						</g>
					</symbol>
				</defs>
				<svg width="100%" height="100%">
					<use id="heartAnimation" href="#heart" width="100%" height="100%" />
				</svg>
			</svg>
		</svg>

		<svg x="20" y="4" width="152" height="152">
			<g transform="translate(50%,50%), rotate(90)" id="clockHand">
				<image x="-76" y="-76" href="clock/hand.png" width="152" height="152" load="sync" />
			</g>
		</svg>

		<use href="#fitfont" id="dateShadow">
