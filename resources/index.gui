			<use href="#fitfont-char"/>
			<use href="#fitfont-char"/>
			<use href="#fitfont-char"/>
		</use>
		<use href="#fitfont" id="date">
			<use href="#fitfont-char"/>
			<use href="#fitfont-char"/>
			<use href="#fitfont-char"/>
			<use href="#fitfont-char"/>
			<use href="#fitfont-char"/>
			<use href="#fitfont-char"/>
			<use href="#fitfont-char"/>
